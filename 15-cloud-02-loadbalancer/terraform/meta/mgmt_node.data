#cloud-config
users:
  - name: vagrant
    groups: sudo
    shell: /bin/bash
    sudo: ['ALL=(ALL) NOPASSWD:ALL']
    ssh-authorized-keys:
      - ssh-rsa  root@vagrant
      - ssh-rsa  vagrant@vagrant
      - ssh-rsa  vagrant@vagrant


package_update: true
packages:
  - mc
  - bash-completion
  - vim
